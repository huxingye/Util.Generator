@* Ui层项目 - npm配置文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    var name = service.GetClientAppName();
    Model.Output.FileNameNoExtension = "package";
    Model.Output.Extension = ".json";
    Model.Output.RelativeRootPath = $"src/{service.GetUiProjectName()}/ClientApp";
}
{
  "name": "@name",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5000",
    "build": "npm run ng-high-memory build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@@angular/cli/bin/ng",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint \"src/**/*.less\" --fix"
  },
  "private": true,
  "dependencies": {
    "@@angular/animations": "~@NpmVersion.Angular",
    "@@angular/common": "~@NpmVersion.Angular",
    "@@angular/compiler": "~@NpmVersion.Angular",
    "@@angular/core": "~@NpmVersion.Angular",
    "@@angular/forms": "~@NpmVersion.Angular",
    "@@angular/platform-browser": "~@NpmVersion.Angular",
    "@@angular/platform-browser-dynamic": "~@NpmVersion.Angular",
    "@@angular/router": "~@NpmVersion.Angular",
    "@@angular-architects/module-federation": "^@NpmVersion.AngularArchitectsModuleFederation",
    "angular-oauth2-oidc": "^@NpmVersion.AngularOauthOidc",
    "rxjs": "~@NpmVersion.Rxjs",
    "tslib": "^@NpmVersion.Tslib",
    "zone.js": "~@NpmVersion.Zonejs",
    "ng-zorro-antd": "^@NpmVersion.NgZorro",
    "ng-alain": "^@NpmVersion.NgAlain",
    "@@delon/abc": "^@NpmVersion.Delon",
    "@@delon/acl": "^@NpmVersion.Delon",
    "@@delon/auth": "^@NpmVersion.Delon",
    "@@delon/cache": "^@NpmVersion.Delon",
    "@@delon/form": "^@NpmVersion.Delon",
    "@@delon/mock": "^@NpmVersion.Delon",
    "@@delon/theme": "^@NpmVersion.Delon",
    "@@delon/util": "^@NpmVersion.Delon",
    "@@delon/chart": "^@NpmVersion.Delon",
    "ajv": "^@NpmVersion.Ajv",
    "ajv-formats": "^@NpmVersion.AjvFormats",
    "screenfull": "^@NpmVersion.Screenfull",
    "lodash": "~@NpmVersion.Lodash",
    "moment": "~@NpmVersion.Moment",
    "util-angular": "^@NpmVersion.UtilAngular"
  },
  "devDependencies": {
    "@@angular-devkit/build-angular": "~@NpmVersion.Angular",
    "@@angular/cli": "~@NpmVersion.Angular",
    "@@angular/compiler-cli": "~@NpmVersion.Angular",
    "@@angular/language-service": "~@NpmVersion.Angular",
    "@@types/jasmine": "~@NpmVersion.TypesJasmine",
    "@@types/node": "^@NpmVersion.TypesNode",
    "@@delon/testing": "^@NpmVersion.Delon",
    "typescript": "~@NpmVersion.Typescript",
    "ngx-build-plus": "^@NpmVersion.NgxBuildPlus",
    "jasmine-core": "~@NpmVersion.JasmineCore",
    "karma": "~@NpmVersion.Karma",
    "karma-chrome-launcher": "~@NpmVersion.KarmaChromeLauncher",
    "karma-coverage": "~@NpmVersion.KarmaCoverage",
    "karma-jasmine": "~@NpmVersion.KarmaJasmine",
    "karma-jasmine-html-reporter": "~@NpmVersion.KarmaJasmineHtmlReporter",
    "@@angular-eslint/builder": "~@NpmVersion.AngularEslint",
    "@@angular-eslint/eslint-plugin": "~@NpmVersion.AngularEslint",
