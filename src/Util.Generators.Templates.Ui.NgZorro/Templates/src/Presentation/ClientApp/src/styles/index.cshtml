@* Ui层项目 - 自定义样式索引文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = "index";
    Model.Output.Extension = ".less";
    Model.Output.RelativeRootPath = $"src/{service.GetUiProjectName()}/ClientApp/src/styles";
}
//表格选中行背景色
.table-row-selected {
    .ant-table-cell {
        background-color: rgb(230,230,240) !important;
    }
    .ant-btn-dangerous {
        background-color: transparent;
    }
}