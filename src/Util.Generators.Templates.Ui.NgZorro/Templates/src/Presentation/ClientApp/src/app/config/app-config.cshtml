@* Ui层项目 - 应用配置文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = "app-config";
    Model.Output.Extension = ".ts";
    Model.Output.RelativeRootPath = $"src/{service.GetUiProjectName()}/ClientApp/src/app/config";
}
import { AppConfig } from 'util-angular';
import { urlConfig } from './url-config';

/**
* 应用配置
*/
export const appConfig: AppConfig = {
    /**
    * Api端点地址
    */
    apiEndpoint: urlConfig.apiEndpointUrl
}