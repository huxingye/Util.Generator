@* Ui层项目 - 编辑页文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = "Edit";
    Model.Output.Extension = ".cshtml";
    Model.Output.RelativeRootPath = $"src/{service.GetUiPagesPath()}";
}
@if ( service.IsTreeEntity() ) {
    @Html.PartialAsync("~/Templates/src/Presentation/Pages/Routes/Edit/Edit.Tree.Part.cshtml", Model)
}
else {
    @Html.PartialAsync("~/Templates/src/Presentation/Pages/Routes/Edit/Edit.Part.cshtml", Model)
}
