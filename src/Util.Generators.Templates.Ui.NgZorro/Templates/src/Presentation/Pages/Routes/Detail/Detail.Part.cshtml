@* Ui层项目组件导入文件 *@
@using Util.Generators.Helpers
@model Util.Generators.Contexts.EntityContext
@{
    var service = new GenerateService( Model );
    Model.Output.FileNameNoExtension = "Detail";
    Model.Output.Extension = ".cshtml";
    Model.Output.RelativeRootPath = $"src/{service.GetUiProjectName()}/Pages/Routes/{service.GetClientAppTitleName()}/{service.EntityName}";
}
@("@page")
@("@model") @(service.EntityName)Dto

@*标题*@
<util-page-header2 title="@(service.EntityDescription)详情" auto-breadcrumb="false"></util-page-header2>
@*表单*@
<util-card borderless="true">
    <util-descriptions column="2" bordered="true">
        @foreach (var property in Model.Properties)
        {
            if (property.IsKey)
                continue;
            if (property.IsDelete)
                continue;
            if (property.IsExtraProperties)
                continue;
            if (property.IsVersion)
                continue;
        <util-descriptions-item for="@property.Name"></util-descriptions-item>
        }
    </util-descriptions>
</util-card>